<AnchorPane prefHeight="768.0" prefWidth="1024.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.StartMenuController">
   <children>
      <ImageView fitHeight="768.0" fitWidth="1030.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Emotion_Start_Screen.png" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <HBox alignment="BOTTOM_CENTER" layoutX="366.0" layoutY="596.0" prefHeight="53.0" prefWidth="292.0" AnchorPane.bottomAnchor="120.0">
